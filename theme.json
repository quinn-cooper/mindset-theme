{
	"customTemplates": [
		{
			"name": "wp-custom-template-page-without-sidebar",
			"postTypes": [
				"page"
			],
			"title": "Page Without Sidebar"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#404040",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#003c71",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#fff200",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#9e9828",
					"name": "Hover",
					"slug": "hover"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "62.5rem"
		},
		"shadow": {
			"presets": []
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [],
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				},
				{
					"fontFace": [],
					"fontFamily": "\"PT Serif\", serif",
					"name": "PT Serif",
					"slug": "pt-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.13rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "30px"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"color": null,
							"left": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"right": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"style": null,
							"top": {
								"color": "currentColor",
								"style": "solid",
								"width": "2px"
							},
							"width": null
						},
						"color": {
							"background": "var(--wp--preset--color--contrast)",
							"text": "var(--wp--preset--color--base)"
						},
						"shadow": "0px 0px 0px 3px #fff200"
					}
				}
			},
			"core/pullquote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--accent)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "3rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "2.5rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "2.1rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "1.8rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "1.5rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontSize": "1rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--hover)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "1rem",
				"right": "1rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}